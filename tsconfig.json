{
    "compilerOptions": {
      "module": "ESNext",
      "declaration": true,
      "noImplicitAny": false,
      "removeComments": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "jsx": "preserve",
      "noLib": false,
      "target": "es6",
      "sourceMap": true,
      "lib": [
        "ESNext",
      ],
      "allowSyntheticDefaultImports": true
    },
    "exclude": [
      "node_modules"
    ]
}
   